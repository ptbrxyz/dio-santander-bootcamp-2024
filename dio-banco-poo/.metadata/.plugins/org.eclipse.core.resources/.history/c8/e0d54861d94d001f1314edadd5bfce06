package com.banco;

public class Main {

	public static void main(String[] args) {
		
		// Usar Conta ou IConta:
		Conta cc = new ContaCorrente();
		Conta poupanca = new ContaPoupanca();
		
		cc.depositar(100);
		cc.imprimirExtrato();
		
		cc.transferir(100, poupanca);
		
		cc.imprimirExtrato();
		poupanca.imprimirExtrato();
	}

}
